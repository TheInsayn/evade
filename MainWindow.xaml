<Window x:Class="WpfEvade.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfEvade"
        mc:Ignorable="d"
        Title="Evade" Height="640" Width="800" ResizeMode="NoResize">
    <Grid>
        <Canvas x:Name="cnvGame" Background="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" Margin="0,0,200,0" Width="600" Height="624"/>
        <Label Content="NAME" HorizontalAlignment="Left" Margin="675,44,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lPlayerName" Content="name" HorizontalAlignment="Left" Margin="675,75,0,0" VerticalAlignment="Top"/>
        <Label Content="TIME" HorizontalAlignment="Left" Margin="675,178,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lTime" Content="0.0s" HorizontalAlignment="Left" Margin="675,209,0,0" VerticalAlignment="Top"/>
        <Label Content="HIGHSCORE" HorizontalAlignment="Left" Margin="678,477,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lHighscoreName" Content="name" HorizontalAlignment="Left" Margin="678,508,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lHighscoreTime" Content="0.0s" HorizontalAlignment="Left" Margin="678,539,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
